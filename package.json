{
  "name": "kenpo-data-entry",
  "version": "0.1.0",
  "description": "data entry tool for the mykenpo database",
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "main": "server.js",
  "scripts": {
    "test": "tape -r babel-register -r css-modules-require-hook/preset \"src/**/*.test.js\" | faucet",
    "lint:js": "eslint src tools",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,sss}\"",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix src tools && stylefmt -r \"src/**/*.{css,less,scss,sss}\"",
    "lint": "npm run lint:js && npm run lint:css",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "stats": "babel-node tools/run stats",
    "start": "babel-node tools/run start",
    "serve": "babel-node tools/run runServer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WintermuteConsulting/kenpo-data-entry.git"
  },
  "contributors": [
    "Ben Brunk",
    "Andrew Falconer"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WintermuteConsulting/kenpo-data-entry/issues"
  },
  "homepage": "https://github.com/WintermuteConsulting/kenpo-data-entry#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "express": "^4.14.0",
    "isomorphic-style-loader": "^1.1.0",
    "mongodb": "^2.2.20",
    "openurl": "^1.1.1",
    "pug": "^2.0.0-beta11",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "string-hash": "^1.1.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.1",
    "babel-plugin-rewire": "^1.0.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "browser-sync": "^2.18.8",
    "chokidar": "^1.6.1",
    "css-loader": "^0.26.1",
    "css-modules-require-hook": "^4.0.5",
    "editorconfig-tools": "^0.1.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "front-matter": "^2.1.2",
    "glob": "^7.1.1",
    "isomorphic-style-loader": "^1.1.0",
    "lint-staged": "^3.3.0",
    "markdown-it": "^8.2.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mustache": "^2.3.0",
    "mz": "^2.6.0",
    "pixrem": "^3.0.2",
    "pleeease-filters": "^3.0.1",
    "postcss": "^5.2.12",
    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^3.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-flexbugs-fixes": "^2.1.0",
    "postcss-loader": "^1.3.0",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nested": "^1.0.0",
    "postcss-nesting": "^2.3.1",
    "postcss-partial-import": "^3.1.0",
    "postcss-pseudoelements": "^3.0.0",
    "postcss-selector-matches": "^2.0.5",
    "postcss-selector-not": "^2.0.0",
    "postcss-url": "^5.1.2",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "source-map-support": "^0.4.11",
    "style-loader": "^0.13.1",
    "stylefmt": "^5.1.2",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0",
    "write-file-webpack-plugin": "^3.4.2"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "plugins": [],
    "globals": {
      "__DEV__": true
    },
    "env": {
      "browser": true
    },
    "rules": {
      "import/extensions": "off",
      "import/no-extraneous-dependencies": "off",
      "react/jsx-filename-extension": "off",
      "react/prefer-stateless-function": "off"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ],
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{css,less,scss,sss}": [
      "stylefmt",
      "stylelint"
    ]
  }
}

  "bugs": {
    "url": "https://github.com/WintermuteConsulting/kenpo-data-entry/issues"
  },
  "homepage": "https://github.com/WintermuteConsulting/kenpo-data-entry#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "mongodb": "^2.2.20",
    "openurl": "^1.1.1",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "node-sass": "^4.5.0"
  }
}
